{
  "name": "front",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "typecheck": "vue-tsc --noEmit",
    "build": "bun run typecheck && vite build",
    "build:staging": "vite build --mode staging",
    "build:production": "SOURCE_MAP=1 vite build --mode production",
    "preview": "bun run build && vite preview --port 8080",
    "format": "prettier ./src --check",
    "format:fix": "prettier ./src --write",
    "lint": "eslint --ext .ts,.tsx,.vue ./src",
    "lint:fix": "bun run lint --fix",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "bun run typecheck && build-storybook",
    "buf-gen": "rm -rf src/infrastructure/api/gen && buf generate --template buf.gen.yaml ../proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "@connectrpc/connect": "^1.6.1",
    "@connectrpc/connect-web": "^1.6.1",
    "@gtm-support/vue-gtm": "^2.2.0",
    "@sentry/rollup-plugin": "^0.4.0",
    "@sentry/tracing": "^7.120.3",
    "@sentry/vue": "^7.120.3",
    "@types/lodash": "^4.17.16",
    "@unocss/reset": "^0.50.8",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.3.1",
    "async-mutex": "^0.5.0",
    "axios": "^1.8.4",
    "core-js": "^3.41.0",
    "dayjs": "^1.11.13",
    "firebase": "^8.10.1",
    "lodash": "^4.17.21",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-click-away": "^1.2.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.51.0",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@connectrpc/protoc-gen-connect-es": "^1.6.1",
    "@rushstack/eslint-patch": "^1.11.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/vue3": "^6.5.16",
    "@types/jest": "29.4.0",
    "@types/node": "^18.19.85",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/compiler-sfc": "3.2.47",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "esbuild-jest": "0.5.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "^3.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "9.15.0",
    "jest": "29.4.2",
    "prettier": "2.2.1",
    "rollup": "^3.29.5",
    "sass": "1.62.1",
    "storybook-builder-vite": "^0.1.23",
    "typescript": "^4.9.5",
    "vite": "^4.5.11",
    "vite-plugin-fonts": "^0.7.0",
    "vue-eslint-parser": "9.1.0",
    "vue-tsc": "^1.8.27"
  }
}
