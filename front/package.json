{
	"name": "front",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"dev": "vite",
		"typecheck": "vue-tsc --noEmit",
		"build": "bun run typecheck && vite build",
		"build:staging": "vite build --mode staging",
		"build:production": "vite build --mode production",
		"preview": "bun run build && vite preview --port 8080",
		"check": "bunx biome check --write .",
		"check:unsafe": "bunx biome check --write --unsafe .",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "bun run typecheck && build-storybook",
		"buf-gen": "rm -rf src/infrastructure/api/gen && buf generate --template buf.gen.yaml ../proto"
	},
	"dependencies": {
		"@bufbuild/protobuf": "^1.8.0",
		"@connectrpc/connect": "^1.4.0",
		"@connectrpc/connect-web": "^1.4.0",
		"@gtm-support/vue-gtm": "^2.0.0",
		"@sentry/rollup-plugin": "^0.4.0",
		"@sentry/tracing": "^7.16.0",
		"@sentry/vue": "^7.16.0",
		"@types/lodash": "^4.14.184",
		"@types/qs": "^6.9.6",
		"@unocss/reset": "^0.50.1",
		"@vueuse/core": "^9.12.0",
		"@vueuse/head": "^1.0.24",
		"async-mutex": "^0.5.0",
		"axios": "^1.4.0",
		"core-js": "^3.33.0",
		"dayjs": "^1.10.4",
		"firebase": "^8.3.2",
		"lodash": "^4.17.21",
		"qs": "^6.10.1",
		"vue": "^3.2.45",
		"vue-router": "4.1.6",
		"vue3-click-away": "^1.2.4",
		"vuedraggable": "^4.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"@bufbuild/buf": "^1.30.0",
		"@bufbuild/protoc-gen-es": "^1.8.0",
		"@connectrpc/protoc-gen-connect-es": "^1.4.0",
		"@storybook/addon-actions": "^6.5.12",
		"@storybook/addon-essentials": "^6.4.19",
		"@storybook/addon-links": "^6.4.19",
		"@storybook/vue3": "^6.4.19",
		"@types/jest": "29.4.0",
		"@types/node": "^18.14.1",
		"@vitejs/plugin-vue": "4.0.0",
		"@vue/compiler-sfc": "3.2.47",
		"@vue/tsconfig": "^0.1.3",
		"esbuild-jest": "0.5.0",
		"jest": "29.4.2",
		"lint-staged": "^13.2.2",
		"rollup": "^3.15.0",
		"sass": "1.62.1",
		"storybook-builder-vite": "^0.1.21",
		"typescript": "^4.6.2",
		"vite": "4.3.9",
		"vite-plugin-fonts": "^0.7.0",
		"vue-tsc": "^1.0.24"
	}
}
